    "lint": "eslint --max-warnings 0 ."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bufbuild/buf": "^1.35.1",
    "@bufbuild/protobuf": "^2.0.0",
    "@bufbuild/protoc-gen-es": "^2.0.0",
    "@bufbuild/protoplugin": "^2.0.0",
    "typescript": "^5.5.4",
    "tsx": "^4.16.1",
    "@types/node": "^22.0.0"
  }
}
