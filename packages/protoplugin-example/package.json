{
  "name": "protoc-gen-twirp-es",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "app:link": "npm link ../protoplugin ../protobuf ../protoc-gen-es",
    "app:buf:generate": "npm run app:link && buf generate buf.build/bufbuild/eliza",
    "app:start": "npm run app:buf:generate && npm run app:build",
    "app:build": "../../node_modules/typescript/bin/tsc --noEmit && npx esbuild app/src/index.ts --serve=localhost:3000 --servedir=app/www --outdir=app/www --bundle --global-name=eliza",
    "plugin:link": "npm link ../protoplugin ../protobuf ../protoc-gen-es",
    "plugin:clean": "rm -rf ./dist/cjs/*",
    "plugin:build": "npm run plugin:link && ../../node_modules/typescript/bin/tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs"
  },
  "author": "Buf",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufbuild/protobuf-es.git",
    "directory": "packages/protoplugin-example"
  },
  "bin": {
    "protoc-gen-twirp-es": "bin/protoc-gen-twirp-es"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^0.1.1",
    "@bufbuild/protoc-gen-es": "^0.1.1",
    "@bufbuild/protoplugin": "^0.1.1"
  }
}
