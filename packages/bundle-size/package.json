    "@bufbuild/protoc-gen-es": "2.0.0-beta.3",
    "@types/brotli": "^1.3.4",
    "brotli": "^1.3.3",
    "esbuild": "^0.22.0",
    "google-protobuf": "3.21.2"
  }
}
